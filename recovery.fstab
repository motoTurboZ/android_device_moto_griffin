# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

# mount point   fstype     device                       device2                  flags
/system         ext4       /dev/block/bootdevice/by-name/system
/system_image   emmc       /dev/block/bootdevice/by-name/system
/data           f2fs       /dev/block/bootdevice/by-name/userdata                flags=encryptable=/dev/block/bootdevice/by-name/metadata
/keymaster      emmc       /dev/block/bootdevice/by-name/keymaster
/keymasterbak   emmc       /dev/block/bootdevice/by-name/keymasterbak
/cache          ext4       /dev/block/bootdevice/by-name/cache
/recovery       emmc       /dev/block/bootdevice/by-name/recovery
/recovery_bak   emmc       /dev/block/bootdevice/by-name/recovery                flags=display="Recovery";backup=1
/boot           emmc       /dev/block/bootdevice/by-name/boot
/misc           emmc       /dev/block/bootdevice/by-name/misc
/firmware       ext4       /dev/block/bootdevice/by-name/modem                   flags=fsflags=ro,nosuid,nodev,barrier=0;mounttodecrypt
/persist        ext4       /dev/block/bootdevice/by-name/persist                 flags=fsflags=noatime,nosuid,nodev,barrier=1,noauto_da_alloc;mounttodecrypt
/fsg            ext4       /dev/block/bootdevice/by-name/fsg
/efs1           emmc       /dev/block/bootdevice/by-name/modemst1                flags=backup=1;display="EFS"
/efs2           emmc       /dev/block/bootdevice/by-name/modemst2                flags=backup=1;subpartitionof=/efs1
/bt_firmware    ext4       /dev/block/bootdevice/by-name/bluetooth               flags=backup=1;display="Bluetooth"
/oem            ext4       /dev/block/bootdevice/by-name/oem
/dsp            ext4       /dev/block/bootdevice/by-name/dsp

/external_sd    auto       /dev/block/mmcblk0p    /dev/block/mmcblk0p1           flags=display="Micro SDCard";storage;wipeingui;removable
/usb-otg        auto       /dev/block/sde1        /dev/block/sde                 flags=display="USB OTG";storage;wipeingui;removable
